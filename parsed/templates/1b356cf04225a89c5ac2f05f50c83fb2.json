{
  "aoc-custom/components/common/navigation": "<nav class=\"navUser\">\n\t<div class=\"container\">\n\t\t{{> components/common/currency-selector}}\n\t\t<div class=\"top-header-contact\">\n\t\t\t<a href=\"mailto: info@wisconsinmade.com\"><i style=\"margin-left:10px;\" class=\"fa fa-envelope\" ></i></i> info@wisconsinmade.com</a>\n\t\t</div>\n\t\t<ul class=\"navUser-section navUser-section--alt\">\n\t\t\t\n\t\t\t{{#if theme_settings.social_icon_placement_top}}\n\t\t\t\t<li class=\"navUser-item navUser-item--social\">\n\t\t\t\t\t{{> components/common/social-links}}\n\t\t\t\t</li>\n\t\t\t \n\t\t\t{{/if}}\n\n\t\t</ul>\n\t </div>\n</nav>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n",
  "components/common/social-links": "{{#if social_media}}\n\n    <ul class=\"socialLinks socialLinks--alt\">\n            <li class=\"socialLinks-item\">\n\t\t\t\t<p>Follow us:</p>\n\t\t\t</li>\n            <li class=\"socialLinks-item\">\n                <a href=\"https://www.facebook.com/Wisconsinmade/\" target=\"_blank\" rel=\"noopener\">\n                    <i class=\"fa fa-facebook-f\"></i>\n                </a>\n            </li>\n            <li class=\"socialLinks-item\">\n                <a href=\"https://www.instagram.com/wisconsinmade/\" target=\"_blank\" rel=\"noopener\">\n                    <i class=\"fa fa-instagram\"></i>\n                </a>\n            </li>\n            <li class=\"socialLinks-item\">\n                <a href=\"https://www.linkedin.com/company/wisconsinmade.com\" target=\"_blank\" rel=\"noopener\">\n                    <i class=\"fa fa-linkedin\"></i>\n                </a>\n            </li>\n            <li class=\"socialLinks-item\">\n                <a href=\"https://www.pinterest.com/wisconsinmade/\" target=\"_blank\" rel=\"noopener\">\n                    <i class=\"fa fa-pinterest-p\"></i>\n                </a>\n            </li>\n            <li class=\"socialLinks-item\">\n                <a href=\"https://twitter.com/wisconsinmade\" target=\"_blank\" rel=\"noopener\">\n                    <i class=\"fa fa-twitter\"></i>\n                </a>\n            </li>\n    </ul>\n{{/if}}\n"
