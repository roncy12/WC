{
  "components/common/footer": "{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}}\n<div class=\"before-footer\">\n    <div class=\"newsletter-footer\">\n                {{#if settings.show_newsletter_box}}\n                    {{> aoc-custom/components/common/subscription-form}}\n                {{/if}}\n\t</div>\n</div>\n<footer class=\"footer\" role=\"contentinfo\">\n    <h2 class=\"footer-title-sr-only\">{{lang 'footer.title'}}</h2>\n    <div class=\"container\">\n\n        <section class=\"footer-info\">\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h3>\n                <ul class=\"footer-info-list\">\n                    {{#each pages}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li>\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\n                    </li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--medium\" data-section-type=\"footer-categories\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h3>\n                <ul class=\"footer-info-list ft-categ\">\n                    {{#each categories}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                </ul>\n            </article>\n\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h3>\n                <ul class=\"footer-info-list\">\n                    {{#each shop_by_brand}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                </ul>\n            </article>\n            {{/and}}\n\n            {{#if settings.address}}\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\n                <h3 class=\"footer-info-heading\">{{lang 'footer.info'}}</h3>\n\t\t\t\t<p><b>Corporate Office:</b></p>\n                <address>{{nl2br settings.address}}</address>\n\t\t\t\t<br><br>\n\t\t\t\t<p><b>Email:</b>  <a href=\"mailto:info@wisconsinmade.com\">info@wisconsinmade.com</a></p>\n                 <p><b>Phone:</b>  <a href=\"tel:8779476233\">877-947-6233</a></p>\n            </article>\n            {{/if}}\n\n\t\t\t<article class=\"footer-info-col footer-info-col--small payment-icons\">\n\t\t\t\t<img src=\"https://cdn11.bigcommerce.com/s-mh7fmcvlrc/content/footer/footer-payment.png\" />\n            </article>\n\n        </section>\n        {{#if theme_settings.geotrust_ssl_common_name}}\n            <div class=\"footer-geotrust-ssl-seal\">\n                {{> components/common/geotrust-ssl-seal}}\n            </div>\n        {{/if}}\n\n\n    </div>\n</footer>\n        {{#if theme_settings.show_copyright_footer}}\n            <div class=\"footer-copyright\">\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n            </div>\n        {{/if}}\n",
  "aoc-custom/components/common/subscription-form": "<div class=\"container\">\n\t<div class=\"newsletter-headings\">\n\t\t<img src=\"https://cdn11.bigcommerce.com/s-mh7fmcvlrc/content/footer/newsletter-env.png\" />\n\t\t\n\t\t<div >\n\t\t\t<h3>{{lang 'newsletter.subscribe'}}</h3>\n\t\t\t<p>Sent Mondays, Thursdays, and Saturday mornings</p>\n\t\t</div>\n\t\t\n\t</div>\n\n\t<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n\t\t<fieldset class=\"form-fieldset\">\n\t\t\t<input type=\"hidden\" name=\"action\" value=\"subscribe\">\n\t\t\t<input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n\t\t\t<input type=\"hidden\" name=\"check\" value=\"1\">\n\t\t\t<div class=\"form-field\">\n\t\t\t\t<label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n\t\t\t\t<div class=\"form-prefixPostfix wrap\">\n\t\t\t\t\t<input class=\"form-input newsletter-inp\"\n\t\t\t\t\t\t   id=\"nl_email\"\n\t\t\t\t\t\t   name=\"nl_email\"\n\t\t\t\t\t\t   type=\"email\"\n\t\t\t\t\t\t   value=\"{{customer.email}}\"\n\t\t\t\t\t\t   placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n\t\t\t\t\t\t   aria-describedby=\"alertBox-message-text\"\n\t\t\t\t\t\t   aria-required=\"true\"\n\t\t\t\t\t>\n\t\t\t\t\t<input class=\"button button--primary form-prefixPostfix-button--postfix newsletter-btn\"\n\t\t\t\t\t\t   type=\"submit\"\n\t\t\t\t\t\t   value=\"{{lang 'newsletter.subscribe_submit'}}\"\n\t\t\t\t\t>\n\t\t\t\t</div>\n\t\t\t\t{{#if settings.show_newsletter_summary }}\n\t\t\t\t\t<div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n\t\t\t\t{{/if}}\n\t\t\t</div>\n\t\t</fieldset>\n\t</form>\n</div>\n\t<script>\n\t\tfunction _initPlaceHolder(){\n\t\t\tif(jQuery(window).width() <= 560){\n\t\t\t\tjQuery('#nl_email').attr('placeholder',\"Subscribe to our Newsletter\");\n\t\t\t}else{\n\t\t\t\tjQuery('#nl_email').attr('placeholder',\"{{lang 'newsletter.email_placeholder'}}\");\n\t\t\t}\n\t\t}\n\t\t_initPlaceHolder();\n\t\tjQuery( window ).resize(function() {\n\t\t\t_initPlaceHolder();\n\t\t});\n\t</script>",
  "components/common/geotrust-ssl-seal": "<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n"
